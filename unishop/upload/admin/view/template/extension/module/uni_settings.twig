							</div>
							<div class="row row-flex">
								<div class="col-sm-2">{{ entry_dadata_status_quickorder }}</div>
								<div class="col-sm-10"><label><input type="checkbox" name="uni_set[dadata][quickorder][status]" value="1" {{ set.dadata.quickorder.status ? 'checked="checked"' }} /></label></div>
							</div>
							<div class="row row-flex">
								<div class="col-sm-2">{{ entry_dadata_status_checkout }}</div>
								<div class="col-sm-10"><label><input type="checkbox" name="uni_set[dadata][checkout][status]" value="1" {{ set.dadata.checkout.status ? 'checked="checked"' }} /></label></div>
							</div>
						</div>
					</div>
				</div>
				{% if is_trial %}
					<div id="tab-key" class="tab-pane">
						<ul class="nav nav-tabs">
							<li class="active"><a href="#tab-key1" data-toggle="tab">{{ tab_main }}</a></li>
						</ul>
						<div id="module" class="tab-content">
							<div id="tab-key1" class="tab-pane active">
								<div class="col-sm-12">{{ heading_activate_manual }}</div>
								<div class="row row-flex">
									<div class="col-xs-12">
										<p>{{ text_how_get_license }}</p>
										<a href="{{ license_url }}" target="_blank" class="btn btn-primary"><b>{{ button_get_license }}</b></a>
									</div>
								</div>
								<div class="row row-flex">
									<div class="col-xs-12">
										<h4>{{ text_i_have_key }}</h4>
										<p>{{ text_i_have_key_p }}</p>
										<div><button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addKey2(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate }}</b></button></div>
										<br />
										<p>{{ text_activate_manual }}</p>
									</div>
								</div>
								<div class="row row-flex">
									<div class="col-xs-12">
										<h4>{{ text_activate_yourself }}</h4>
										<p>{{ text_activate_yourself_p }}</p>
										<div class="full">
											<input type="text" name="key" value="" class="form-control" style="width:500px;max-width:100%;margin:5px 0 20px !important;" placeholder="{{ placeholder_lic_key }}" />
											<button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addKey(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate }}</b></button>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				{% endif %}
			</div>
			<input type="hidden" name="uni_set[version]" value="{{ version }}" />
			<input type="hidden" name="uni_set[save_date]" value="" />
			<input type="hidden" name="uni_set[sort_stories]" value="{{ set[sort_stories] ? set[sort_stories] : 'url,asc' }}" />
			<input type="hidden" name="store_id" value="{{ store_id }}" />
		</form>
	{% else %}
		<div id="module" style="padding:20px;background:#fff;border-radius:4px">
			{% if trial_end %}
				<h4><b>{{ text_trial_end }}</b></h4>
				<hr />
				<div style="line-height:1.7em">
					<p>{{ text_how_get_license }}</p>
					<a href="{{ license_url }}" target="_blank" class="btn btn-primary"><b>{{ button_get_license }}</b></a>
				</div>
				<hr />
				<h4>{{ text_i_have_key }}</h4>
				<div style="line-height:1.7em">
					<p>{{ text_i_have_key_p }}</p>
					<div><button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addKey2(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate }}</b></button></div>
					<br />
					<p>{{ text_activate_manual }}</p>
				</div>
				<hr />
				<h4>{{ text_activate_yourself }}</h4>
				<div style="line-height:1.7em">
					<p>{{ text_activate_yourself_p }}</p>
					<input type="text" name="key" value="" class="form-control" style="width:500px;max-width:100%;margin:5px 0 20px !important;" placeholder="{{ placeholder_lic_key }}" />
					<button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addKey(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate }}</b></button>
				</div>
			{% elseif key_empty %}
				<h4><b>{{ text_welcome_title }}</b></h4>
				<p>{{ text_welcome }}</p>
				<div class="overflow" style="display:block;max-width:1200px;height:320px;max-height:100%;margin:10px 0 5px;color:#666;font-size:1.0em;font-weight:400">
					{{ license_trial }}
				</div>
				<div id="trial">
					<div style="height:10px"></div>
					<label><input type="checkbox" name="trial" value="1" />{{ text_agree }}</label>
					<div style="height:5px"></div>
					<button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addTrial(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate_trial }}</b></button>
				</div>
				<hr />
				<h4>{{ text_activate_yourself }}</h4>
				<p>{{ text_activate_yourself_p_1 }}</p>
				<input type="text" name="key" value="" class="form-control" style="width:500px;max-width:100%;margin:5px 0 20px !important;" placeholder="{{ placeholder_lic_key }}" />
				<button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addKey(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate }}</b></button>
			{% else %}
				<div style="line-height:1.7em">
					{{ text_license_is_corrupt }}
				</div>
				<br />
				<input type="text" name="key" value="" class="form-control" style="width:500px;max-width:100%;margin:0 0 20px !important;" placeholder="{{ placeholder_lic_key }}" />
				<button type="button" class="btn btn-primary" style="background:#f2766b" onclick="addKey(this);" data-loading-text="<i class='fa fa-spinner fa-spin'></i>"><b>{{ button_activate }}</b></button>
			{% endif %}
		</div>
	{% endif %}
	</div>
	<div class="popup_icons"><div class="fontawesome-icon-list"></div></div>
</div>
<div class="info-news-list" style="display:none">
