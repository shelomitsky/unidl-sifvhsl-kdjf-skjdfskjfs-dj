		<li style="padding:5px 20px 5px;font-size:1.1em;color:#e66761"><b>{{ text_info }}</b></li>
		{% for info in informations.articles %}
			<li><a data-name="{{ info.name }}" data-href="{{ info.link }}" data-href-seo="{{ info.seolink }}">{{ info.name }}</a></li>
