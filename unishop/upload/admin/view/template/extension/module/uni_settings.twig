{{ header }}
<style>
	@media (min-width:768px) and (max-width:2000px){
		#column-left{width:235px !important}
		#column-left:not(.active){left:-235px !important}
		#column-left:not(.active) + #content, #column-left + #content + #footer {margin-left:0 !important}
		.show-column-left {position:fixed;top:130px;left:0;display:flex;align-items:center;justify-content:center;width:30px;height:40px;color:#fff;background:#444;border-radius:0 4px 4px 0;transition: all .3s;cursor:pointer;z-index:9;opacity:.5}
		.show-column-left.active{left:235px}
		.show-column-left:not(.active) .fa-chevron-left{display:none}
		.show-column-left.active .fa-chevron-right{display:none}
	}
</style>
