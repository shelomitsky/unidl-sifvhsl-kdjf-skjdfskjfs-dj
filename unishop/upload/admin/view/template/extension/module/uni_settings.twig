		<div class="container-fluid">
			{% if sys_info.input_vars < 2000 %}
				<div class="uni-alert" style="margin:0 0 20px;padding:15px;color:#fff;background:#f00;border-radius:4px">
					{{ alert_max_input_vars }} <b>{{ sys_info.input_vars }}</b>
				</div>
			{% endif %}
			<div class="pull-right">
				{% if show_settings %}
				<div class="btns">
					<button data-toggle="tooltip" title="{{ button_apply_settings }}" class="btn btn-success"><i class="fa fa-save"></i></button>
					&nbsp;
					<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
				</div>
				{% endif %}
			</div>
			<h1>{{ heading_title }}</h1>
			{% if version_type %}
				<div class="{{ version_type.type }}">
					{% if version_type.type == 'trial' %}
						<span onclick="$('li.license a').click()">{{ version_type.text }} <a>{{ text_get_license }}</a></span>
					{% else %}
						<span>{{ version_type.text }}</span>
					{% endif %}
				</div>
			{% endif %}
			<br />
			<ul class="breadcrumb">
				{% for breadcrumb in breadcrumbs %}
					<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
				{% endfor %}
			</ul>
		</div>
